   <xs:complexType name="Tutenti">
     <xs:sequence>
     <xs:element name="utente" minOccurs="0" maxOccurs="unbounded" type="Tutente" />
     </xs:sequence>
      </xs:complexType>

    <xs:complexType name="Tutente">
     <xs:sequence>
     <xs:element name="Username" type="xs:string" use="required"/>
     <xs:element name="Password" type="xs:string" use="required"/>
     <xs:element name="Nome" type="xs:string" use="required"/>
     <xs:element name="Cognome" type="xs:string" use="required"/>
     <xs:element name="Sesso" type="Tsesso" use="required"/>
     <xs:element name="DataNascita" type="Tdata" />
     </xs:sequence>
  <xs:attribute name="tipo" type="xs:string" use="required" />
    </xs:complexType>
   
     

     <xs:simpleType name="Tsesso">
      <xs:restriction base="xs:string">
        <xs:enumeration value="Maschio" />
        <xs:enumeration value="Femmina" />
        <xs:enumeration value="Asessuato" />
        <xs:enumeration value="Trans" />
        <xs:enumeration value="CalÃ¹" />
      </xs:restriction>
     </xs:simpleType>
     
     <xs:complexType name="Tdata">
      <xs:sequence>
        <xs:element name="Giorno" type="Tgiorno" use="required"/>
        <xs:element name="Mese" type="Tmese" use="required"/>
        <xs:element name="Anno" type="Tanno" use="required"/>
      </xs:sequence>
     </xs:simpleType>

    
      <xs:simpleType name="Tgiorno">
        <xs:restriction base="xs:integer">
